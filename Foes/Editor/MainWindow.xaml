                        <CheckBox.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FF00B4FC" Offset="0.007"/>
                                <GradientStop Color="#FF00BEFF" Offset="1"/>
                            </LinearGradientBrush>
                        </CheckBox.Background>
                    </CheckBox>
                </Grid>

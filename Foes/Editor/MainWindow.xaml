                                <GradientStop Color="#FF00B4FC" Offset="0.007"/>
                                <GradientStop Color="#FF00BEFF" Offset="1"/>
                            </LinearGradientBrush>
                        </CheckBox.Background>
                    </CheckBox>
                    <Label Content="Humanoid auto scale:" HorizontalAlignment="Left" Height="28" Margin="207,327,0,0" VerticalAlignment="Top" Width="182" Foreground="#FF00B4FC"/>

        <ImageBrush ImageSource="B1.PNG"/>
    </Window.Background>
    <Grid>
        <ListBox x:Name="FoeList" HorizontalAlignment="Left" Height="873" Margin="28,24,0,0" VerticalAlignment="Top" Width="216" Background="{x:Null}" Foreground="#FF00B4FF" SelectionChanged="FoeList_SelectionChanged"/>
